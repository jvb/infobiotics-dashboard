lattice rectangular

	dimension	2
	scale 		10
	units 		microns
	xmin			0
	xmax			10
	ymin			0
	ymax			30

	parameters
		parameter a value = 1
		parameter b value = 10
	endParameters

	basis
		(a,0)
		(0,a)
	endBasis

	vertices
		(a/2,a/2)
		(-a/2,a/2)
		(-a/2,-a/2)
		(a/2,-a/2)
	endVertices

	neighbours
		(1,0)
		(-1,0)
		(0,1)
		(0,-1)
	endNeighbours

endLattice
