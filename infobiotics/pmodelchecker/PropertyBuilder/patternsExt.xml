<pattern class="Instantaneous" id="pp011">
<question value="Probability of the system being in state s1 at time T." />
<template value="Probability of the system being in state #s1 at time #T." />
<var id="#s1" type="state" />
<var id="#T" type="numeric" />
<translationList>
	<translation to="pctl" value="" />
	<translation to="ctl" value="" />
	<translation to="csl" value="P=? [true U[#T,#T] #s1]" />
</translationList>
</pattern>

